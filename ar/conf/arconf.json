    }
  ],
  "httpTomeOut": 2000,
  "log4js": {
    "appenders": {
      "console": {
        "type": "console"
      },
      "log_date":{
        "type": "dateFile",
        "filename": "logs/system.log",
        "alwaysIncludePattern": true,
        "pattern": "-yyyy-MM-dd-hh",
        "encoding": "utf-8"
      }
    },
    "categories": {
      "default": {
        "appenders":["console","log_date"],
        "level": "debug"
      }
    }
  }
}
