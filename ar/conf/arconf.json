{
  "port":3000,
  "polling":true,
  "redisDeployType":"cluster",
  "redisAddress": [
    {
      "port": 6379,
      "host": "172.16.40.4"
    },
    {
      "port": 6380,
      "host": "172.16.40.4"
    },
    {
      "port": 6381,
